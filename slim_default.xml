  <project path="packages/services/Telecomm" name="packages_services_Telecomm" groups="pdk-cw-fs,pdk-fs" remote="slim" />
  <project path="packages/services/Telephony" name="packages_services_Telephony" groups="pdk-cw-fs,pdk-fs" remote="slim" />
  <project path="system/bt" name="android_system_bt" remote="slim" />
  <project path="system/core" name="android_system_core" groups="pdk" remote="slim" />
  <project path="vendor/slim" name="vendor_slim" remote="slim" />

  <!-- Prebuilts -->
  <project path="prebuilts/tools-slim" name="prebuilts_tools-slim" remote="slim" clone-depth="1" />
  <project path="prebuilts/openssl" name="prebuilts_openssl" remote="slim" clone-depth="1" />

</manifest>

